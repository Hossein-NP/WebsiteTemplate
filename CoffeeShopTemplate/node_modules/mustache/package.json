{
  "name": "mustache",
  "version": "4.2.0",
  "description": "Logic-less {{mustache}} templates with JavaScript",
  "author": "mustache.js Authors <http://github.com/janl/mustache.js>",
  "homepage": "https://github.com/janl/mustache.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/janl/mustache.js.git"
  },
  "keywords": [
    "mustache",
    "template",
    "templates",
    "ejs"
  ],
  "main": "mustache.js",
  "bin": {
    "mustache": "./bin/mustache"
  },
  "files": [
    "bin/",
    "mustache.mjs",
    "mustache.min.js",
    "wrappers/"
  ],
  "exports": {
    ".": {
      "import": "./mustache.mjs",
      "require": "./mustache.js"
    },
    "./*": "./*"
  },
  "volo": {
    "url": "https://raw.github.com/janl/mustache.js/{version}/mustache.js"
  },
  "scripts": {
    "build": "cp mustache.js mustache.mjs && rollup mustache.mjs --file mustache.js --format umd --name Mustache && uglifyjs mustache.